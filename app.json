{
  "expo": {
    "name": "qBitRemote",
    "slug": "qBitRemote",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon2.png",
    "privacy": "unlisted",
    "scheme": "qbitremote",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ryanhill.qBitRemote"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ryanhill.qBitRemote"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
